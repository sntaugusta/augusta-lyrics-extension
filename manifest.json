{
  "name": "Augusta Lyrics Extension",
  "description": "Uma extensão do Google Chrome para criar botões de copiar títulos e letras de músicas para a projeção da igreja",
  "version": "2.0.2",
  "manifest_version": 3,
  "icons": {
    "16": "favicon-16.png",
    "32": "favicon-32.png",
    "48": "favicon-48.png",
    "64": "favicon-64.png",
    "80": "favicon-80.png",
    "96": "favicon-96.png",
    "112": "favicon-112.png",
    "128": "favicon-128.png"
  },
  "content_scripts": [
    {
      "js": ["buttons.js"],
      "matches": [
        "https://www.letras.mus.br/*"
      ]
    }
  ]
}
